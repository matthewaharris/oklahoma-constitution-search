var navigation, footer;
function load_css(url){
	links = Array.from(document.querySelectorAll('link')).map(function(item){ return item.href });
	if( 0 >= links.indexOf(url)){
		const link = document.createElement('link');
		link.rel = 'stylesheet';
		link.type = 'text/css';
		link.href = url;
		document.head.appendChild(link);
	}
}
function load_javascript(src){
	scripts = Array.from(document.querySelectorAll('script')).map(function(item){ return item.src; });
	scripts = scripts.filter(function(item){ return 1 >= scripts.filter(function(i){ return i == item }).length })
	
	if( 0 >= scripts.indexOf(src)){
		const script = document.createElement('script');
		script.src = src;
		script.type = 'text/javascript'; // Optional, but recommended
		script.async = false; // Optional, to ensure execution order
		document.head.appendChild(script);
	}
}

function prepare_county_list(){
    var counties = [{ name: 'Adair' , seat: 'Stilwell'},{ name: 'Alfalfa' , seat: 'Cherokee'},{ name: 'Atoka' , seat: 'Atoka'},{ name: 'Beaver' , seat: 'Beaver'},{ name: 'Beckham' , seat: 'Sayre'},{ name: 'Blaine' , seat: 'Watonga'},{ name: 'Bryan' , seat: 'Durant'},{ name: 'Caddo' , seat: 'Anadarko'},{ name: 'Canadian' , seat: 'El Reno'},{ name: 'Carter' , seat: 'Ardmore'},{ name: 'Cherokee' , seat: 'Tahlequah'},{ name: 'Choctaw' , seat: 'Hugo'},{ name: 'Cimarron' , seat: 'Boise City'},{ name: 'Cleveland' , seat: 'Norman'},{ name: 'Coal' , seat: 'Coalgate'},{ name: 'Comanche' , seat: 'Lawton'},{ name: 'Cotton' , seat: 'Walters'},{ name: 'Craig' , seat: 'Vinita'},{ name: 'Creek' , seat: 'Sapulpa'},{ name: 'Custer' , seat: 'Arapaho'},{ name: 'Delaware' , seat: 'Jay'},{ name: 'Dewey' , seat: 'Taloga'},{ name: 'Ellis' , seat: 'Arnett'},{ name: 'Garfield' , seat: 'Enid'},{ name: 'Garvin' , seat: 'Pauls Valley'},{ name: 'Grady' , seat: 'Chickasha'},{ name: 'Grant' , seat: 'Medford'},{ name: 'Greer' , seat: 'Mangum'},{ name: 'Harmon' , seat: 'Hollis'},{ name: 'Harper' , seat: 'Buffalo'},{ name: 'Haskell' , seat: 'Stigler'},{ name: 'Hughes' , seat: 'Holdenville'},{ name: 'Jackson' , seat: 'Altus'},{ name: 'Jefferson' , seat: 'Waurika'},{ name: 'Johnston' , seat: 'Tishomingo'},{ name: 'Kay' , seat: 'Newkirk'},{ name: 'Kingfisher' , seat: 'Kingfisher'},{ name: 'Kiowa' , seat: 'Hobart'},{ name: 'Latimer' , seat: 'Wilburton'},{ name: 'LeFlore' , seat: 'Poteau'},{ name: 'Lincoln' , seat: 'Chandler'},{ name: 'Logan' , seat: 'Guthrie'},{ name: 'Love' , seat: 'Marietta'},{ name: 'Major' , seat: 'Fairview'},{ name: 'Marshall' , seat: 'Madill'},{ name: 'Mayes' , seat: 'Pryor'},{ name: 'McClain' , seat: 'Purcell'},{ name: 'McCurtain' , seat: 'Idabel'},{ name: 'McIntosh' , seat: 'Eufaula'},{ name: 'Murray' , seat: 'Sulphur'},{ name: 'Muskogee' , seat: 'Muskogee'},{ name: 'Noble' , seat: 'Perry'},{ name: 'Nowata' , seat: 'Nowata'},{ name: 'Okfuskee' , seat: 'Okemah'},{ name: 'Oklahoma' , seat: 'Oklahoma City'},{ name: 'Okmulgee' , seat: 'Okmulgee'},{ name: 'Osage' , seat: 'Pawhuska'},{ name: 'Ottawa' , seat: 'Miami'},{ name: 'Pawnee' , seat: 'Pawnee'},{ name: 'Payne' , seat: 'Stillwater'},{ name: 'Pittsburg' , seat: 'McAlester'},{ name: 'Pontotoc' , seat: 'Ada'},{ name: 'Pottawatomie' , seat: 'Shawnee'},{ name: 'Pushmataha' , seat: 'Antlers'},{ name: 'Roger Mills' , seat: 'Cheyenne'},{ name: 'Rogers' , seat: 'Claremore'},{ name: 'Seminole' , seat: 'Wewoka'},{ name: 'Sequoyah' , seat: 'Sallisaw'},{ name: 'Stephens' , seat: 'Duncan'},{ name: 'Texas' , seat: 'Guymon'},{ name: 'Tillman' , seat: 'Frederick'},{ name: 'Tulsa' , seat: 'Tulsa'},{ name: 'Wagoner' , seat: 'Wagoner'},{ name: 'Washington' , seat: 'Bartlesville'},{ name: 'Washita' , seat: 'Cordell'},{ name: 'Woods' , seat: 'Alva'},{ name: 'Woodward' , seat: 'Woodward'}]

	last_alpha = "";
	list = [];
	counties.forEach(function(county){
		alpha = county.name[0]
		county.url = "/courts/"+county.name.toLowerCase().replace(' ','-');
		if(last_alpha != alpha){
			index = { name: alpha, items: [] };
			list.push(index);
		}
		list[list.length-1].items.push(county)
		last_alpha = alpha;
	});

	data = { list: list }
	data_list = {
		list:[
		 {name:'A - B', id:'1', items: [list[0], list[1]] }
		,{name:'C', id:'2', items: [list[2]] }
		,{name:'D - E - F - G', id:'3', items: [list[3],list[4],list[5]] }
		,{name:'H - I - J - K', id:'4', items: [list[6],list[7],list[8]] }
		,{name:'L - M - N', id:'5', items: [list[9],list[10],list[11]] }
		,{name:'O - P - Q - R', id:'6', items: [list[12],list[13],list[14]] }
		,{name:'S - T - U - V - W', id:'7', items: [list[15],list[16],list[17]] }
		]
	}
	
	list_html = data_list.list.map(function(item){
	
		alpha_list = item.items.map(function(alpha){
			lists = alpha.items.map(function(county){
				return `<li><a class="county-link" href="${county.url}"><span class='county-name'>${county.name}</span></a> <span class='county-seat'>${county.seat}</span></li>`;
			});
			return `<h4 class="heading-${alpha.name}">${alpha.name}</h4><ul>${lists.join('')}</ul>`;
		});
	
		return `
		<div>
			<a data-toggle-group="county-list" data-toggle="shrink" href="#alpha-${item.id}" aria-expanded="false" aria-controls="alpha-${item.id}"><span>${item.name}</span></a>
			<div class="shrink county-list" id="alpha-${item.id}">
				${alpha_list.join('')}
			</div>
		</div>
		`;
	});

	result = `${list_html.join('')}`

	document.querySelectorAll('.county-list').forEach(function(element, index){
		element.innerHTML = result.replaceAll("alpha-","alpha-"+index+"-");
	});
	
	function close_all(){
		document.querySelectorAll('.county-list .show').forEach(function(item){
			item.classList.remove('show');
		});
		document.querySelectorAll('.county-list a[aria-expanded="true"]').forEach(function(item){
			item.setAttribute('aria-expanded',false);
		});
	}
	document.querySelectorAll('a[data-toggle="shrink1"]').forEach(function(item){
		item.addEventListener('click', function(e){
			e.preventDefault();
			
			target = e.target;

			if(target.tagName != "A"){
				target = target.closest('a');
			}
			console.log(target);
			link = target.getAttribute('aria-controls');
			accordion = document.getElementById(link);			
			
			console.log(accordion);
			//CLOSE ALL THAT ARE NOT THIS ONE
			temp = Array.from(document.querySelectorAll('.county-list')).filter(function(item){
				return item != accordion;
			});
			temp.filter(function(item){ 
				return item.classList.contains('show');
			}).forEach(function(ele){ 
				ele.classList.toggle('show');
			});
			
			accordion.classList.toggle("show");
			target.setAttribute('aria-expanded','true');
		});
	});	
}

function main_navigation(){
	if(!document.querySelectorAll('.oscn-navigation')[0] && document.querySelectorAll('header')[0]){ //CHECK IF ALREADY LOADED OR IF HEADER EXISTS
		//load_css("/wp-content/themes/oscn-theme/navigation-style.css");
		//load_css("https://pro.fontawesome.com/releases/v5.15.4/css/all.css");
		load_css("https://cdnjs.cloudflare.com/ajax/libs/flexboxgrid/6.3.1/flexboxgrid.min.css");
		
		//LOAD NAVIGATION
		var container = document.querySelectorAll('header')[0];
		container.outerHTML = navigation;
		console.log("NAVIGATION LOADED:");
		
		//LOAD FOOTER
		var footer_container = document.querySelectorAll('footer, .footer')[0];
		if(!footer_container){
			footer_container = document.createElement('div');
			document.querySelectorAll('body')[0].appendChild(footer_container);
		}
		footer_container.outerHTML = footer;
		console.log("FOOTER LOADED");

		//load_javascript('https://cdnjs.cloudflare.com/ajax/libs/mustache.js/4.1.0/mustache.min.js');
		load_javascript('/wp-content/plugins/oscn-navigation-panel-update/js/navigation.js?v=1.0.1');
		//load_javascript('https://admin.oscn.net/wp-content/plugins/oscn-county-list-accordian/js/oscn_county_list_accordian.js');
		document.getElementById('masthead').style.display = "";
		
		prepare_county_list();

		sendRequest("/datafiles/ext/json-alerts/").then(function(response){
			alerts = JSON.parse(response);

			if(alerts && alerts.length >= 1){
				alerts.forEach(function(item, n){
					item.numbering = (n+1)+" of "+alerts.length;
				});
			
				template = document.getElementById("template-alerts").innerHTML;
					
				output = Mustache.render(template,alerts);
				document.querySelectorAll('#site-alert')[0].innerHTML = output;
				document.querySelectorAll('#site-alert')[0].removeAttribute('hidden');
				
				/*****
				document.getElementById("site-alert").addEventListener("click", function(e){
					cycle_alerts();
				});
				cycle_alerts();
				*****/
			}			
		});
	
	}
}

/*LOAD NAVIGATION ON STATE CHANGE*/
document.addEventListener("readystatechange", function(event){
	if (document.readyState === "interactive") {
		main_navigation();
	}
});


